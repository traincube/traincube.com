{
  "name": "traincube.com",
  "version": "1.0.0",
  "description": "Traincube Website",
  "scripts": {
    "prestart": "open http://localhost:8080",
    "start": "npm-run-all --parallel security-check start-server lint:watch test:watch",
    "start-server": "webpack-dev-server -d --config webpack.config.dev.js",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start-server localtunnel",
    "test": "mocha --reporter progress scripts/test.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist":"rimraf ./dist && mkdir dist",
    "prebuild":"npm-run-all clean-dist test lint && open http://localhost:3000",
    "build":"webpack --config webpack.config.prod.js",
    "postbuild":"cd dist && serve"
  },
  "author": "Hendrix Tavarez",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "2.4.1",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "webpack-md5-hash": "0.0.5"
  }
}
